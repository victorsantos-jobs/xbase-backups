# Configurações do Agente Minerador
AGENTE_INTERVALO=3600  # Intervalo em segundos (1 hora)
AGENTE_TIMEOUT=300     # Timeout para operações web
AGENTE_USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36

# Configurações de Busca
BUSCA_MAX_RESULTADOS=100
BUSCA_DELAY_ENTRE_REQUESTS=2

# Configurações de Armazenamento
STORAGE_PATH=./storage
STORAGE_MAX_SIZE_MB=1024

# Configurações de Log
LOG_LEVEL=INFO
LOG_FILE=./logs/minerador.log

# Configurações de Proxy (opcional)
USE_PROXY=false
PROXY_HOST=
PROXY_PORT=
PROXY_USERNAME=
PROXY_PASSWORD=
